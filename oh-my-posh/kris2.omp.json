{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:f-lightgrey",
          "foreground": "transparent",
          "style": "diamond",
          "template": " {{.Icon}} ",
          "trailing_diamond": "\ue0b0",
          "type": "os"
        },
        {
          "background": "p:f-blackgrey",
          "foreground": "p:f-white",
          "leading_diamond": "\ue0b2",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "trailing_diamond": "\ue0b0",
          "type": "executiontime"
        },
        {
          "background": "p:f-azure",
          "foreground": "p:f-black",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uebd8 Subscription {{ .Name }} ({{ if .EnvironmentName | contains \"AzureCloud\" }}{{ \"Global\" }}{{ else }}{{ .EnvironmentName }}{{ end }}) ",
          "type": "az"
        },
        {
          "type": "terraform",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "properties": {
            "fetch_version": true
          },
          "foreground": "p:f-white",
          "background": "p:f-terraform",
          "template": " \udb84\udc62 Workspace: {{ .WorkspaceName }}{{ if .Version }} {{ .Version }}{{ end }} "
        },
        {
          "background": "p:f-blackgrey",
          "foreground": "p:f-yellow",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4 ",
          "type": "python"
        },
        {
          "foreground": "p:f-white",
          "background": "p:f-blackgrey2",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "template": " [.NET] {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "type": "dotnet"
        },
        {
          "background": "p:f-blackgrey2",
          "foreground": "p:f-yellow",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\ue626 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4 ",
          "type": "go"
        },
        {
          "background": "p:f-aws",
          "foreground": "p:f-black",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\ue7ad {{.Profile}}{{if .Region}}@{{.Region}}{{end}}",
          "trailing_diamond": "\ue0b4 ",
          "type": "aws"
        },
        {
          "background": "p:f-blackgrey2",
          "foreground": "p:f-white",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "\uf308 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}",
          "trailing_diamond": "\ue0b4",
          "type": "kubectl"
        },
        {
          "background": "p:f-gitgreen",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:f-blackorange{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:f-blackyellow{{ end }}",
            "{{ if gt .Ahead 0 }}p:f-purple{{ end }}",
            "{{ if gt .Behind 0 }}p:f-purple{{ end }}"
          ],
          "foreground": "p:f-white",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "diamond",
          "template": " {{ .HEAD }}{{ if .Staging.Changed }}<p:f-orange> \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "git"
        },
        {
          "background": "p:f-red",
          "foreground": "p:f-white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "<transparent> \uf12a</> {{ reason .Code }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "transparent",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:f-blackgreen{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:f-blackyellow{{end}}",
            "{{if eq \"Full\" .State.String}}p:f-blackgreen{{end}}"
          ],
          "foreground": "p:f-white",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 \ueb2d ",
          "type": "battery"
        },
        {
          "background": "transparent",
          "foreground": "p:f-white",
          "style": "diamond",
          "template": " \ue266 MEM: {{ round .PhysicalPercentUsed .Precision }}% | {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB \ue266 ",
          "type": "sysinfo"
        },
        {
          "background": "transparent",
          "foreground": "p:f-white",
          "properties": {
            "time_format": "15:04:05"
          },
          "leading_diamond": "\ue0c5",
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "foreground": "p:f-white",
          "style": "plain",
          "template": " \uf292 ",
          "type": "root"
        },
        {
          "foreground": "p:f-lightgrey",
          "properties": {
            "folder_icon": "\uf07b ",
            "folder_separator_icon": " \uf061 ",
            "home_icon": "\ueb06 ",
            "style": "agnoster_full"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u2570\u2500 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "<p:f-white>",
    "template": "\ue285 "
  },
  "palette": {
    "f-main-color": "#01579B",
    "f-white": "#FFFFFF",
    "f-yellow": "#FFEA00",
    "f-blackyellow": "#5C5400",
    "f-black": "#000000",
    "f-orange": "#ff8459",
    "f-blackorange": "#875C28",
    "f-violet": "#a313a8",
    "f-purple": "#B388FF",
    "f-green": "#00D100",
    "f-blackgreen": "#004400",
    "f-gitgreen": "#004400",
    "f-gitgreen2": "#17D7A0",
    "f-blackgreen": "#2C3F1E",
    "f-red": "#910000",
    "f-grey": "#565656",
    "f-blackgrey": "#2E2E2E",
    "f-blackgrey2": "#494949",
    "f-blackgrey3": "#333333",
    "f-lightgrey": "#8C8C8C",
    "f-charcoal": "#303030",
    "f-azure": "#33B2E7",
    "f-aws": "#FF920B",
    "f-terraform": "#7B42BC"
  },
  "version": 3
}
